{
  "report-dir": "coverage-e2e",
  "temp-dir": "coverage-e2e/.nyc_output",
  "reporter": [
    "html",
    "json",
    "json-summary",
    "lcov",
    "text"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.tsx",
    "src/**/__tests__/**",
    "src/**/__stories__/**",
    "src/**/types/**",
    "**/*.d.ts"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "all": true,
  "check-coverage": true,
  "statements": 70,
  "branches": 65,
  "functions": 70,
  "lines": 70
}
