{
    "name": "Net logs",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Web extension for custom network logs representation",
    "devtools_page": "devtools.html",
    "author": "artboomy",
    "icons": {
        "16": "icons/16-1.png",
        "48": "icons/48-1.png",
        "128": "icons/128-1.png"
